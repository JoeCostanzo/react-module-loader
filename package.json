{
  "name": "react-module-loader",
  "version": "1.0.8",
  "description": "Loads ES6 modules (functions, etc) into interactive React component views",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoeCostanzo/react-module-loader.git"
  },
  "author": "Joe Costanzo",
  "license": "MIT",
  "keywords": [
    "react",
    "ES6",
    "modules",
    "loader",
    "jsdoc",
    "AST"
  ],
  "scripts": {
    "prepublish": "npm-run-all styles scripts",
    "buildwatch": "npm-run-all --parallel styleswatch scriptswatch",
    "scripts": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./dist",
    "scriptswatch": "babel --plugins transform-es2015-modules-umd src --watch --ignore __tests__ --out-dir ./dist",
    "styles": "gulp styles",
    "styleswatch": "gulp styles:watch",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "mocha": "mocha --require scripts/mocha_runner src/**/__tests__/**/*.js",
    "test": "npm run lint && npm run mocha",
    "test-watch": "npm run mocha -- --watch --watch-extensions js"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "react-addons-test-utils": "^15.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "sinon": "^1.17.3"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^15.0.0",
    "react-dom": "~0.14.8 || ^15.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "npm-run-all": "^4.0.2"
  }
}
